  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/register": "^7.23.7",
    "@wdio/allure-reporter": "^8.27.2",
    "@wdio/cli": "^8.27.1",
    "@wdio/local-runner": "^8.27.0",
    "@wdio/mocha-framework": "^8.27.0",
    "@wdio/spec-reporter": "^8.27.0",
    "chromedriver": "^120.0.1",
    "allure-commandline": "^2.25.0"
  }
}
