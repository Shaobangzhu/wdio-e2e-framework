{
  "name": "wdio-e2e-test",
  "version": "1.0.0",
  "description": "This is an automation test framework implement using webdriver.io",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "wdio": "wdio run ./wdio.conf.js",
    "automation-test-store-tests": "npx wdio --spec test/specs/automation-test-store/*.spec.js",
    "webdriver-university-tests": "npx wdio --spec test/specs/webdriver-university/*.spec.js",
    "smoke-suite": "npx wdio --suite smoke"
  },
